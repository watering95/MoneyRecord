<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            card_view:cardCornerRadius="8dp"
            card_view:cardBackgroundColor="@color/colorWhiteGreen" card_view:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="2dp" card_view:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="2dp" card_view:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="4dp" card_view:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="4dp">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <TextView
                    android:text="@string/text"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/text_account_num_card_home" android:layout_marginTop="8dp"
                    card_view:layout_constraintTop_toTopOf="parent" card_view:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp"
                    android:textAlignment="center"/>
            <TextView
                    android:text="@string/text"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/text_principal_card_home"
                    card_view:layout_constraintStart_toEndOf="@+id/text_account_num_card_home"
                    android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    card_view:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
                    card_view:layout_constraintBottom_toBottomOf="parent" android:textAlignment="textEnd"/>
            <TextView
                    android:text="@string/text"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/text_evaluation_card_home"
                    android:layout_marginTop="8dp"
                    card_view:layout_constraintTop_toTopOf="parent"
                    card_view:layout_constraintStart_toEndOf="@+id/text_principal_card_home"
                    android:layout_marginStart="8dp" android:textAlignment="textEnd"/>
            <TextView
                    android:text="@string/text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/text_rate_card_home"
                    android:layout_marginTop="8dp"
                    card_view:layout_constraintTop_toTopOf="parent"
                    card_view:layout_constraintStart_toEndOf="@+id/text_evaluation_card_home"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp" card_view:layout_constraintEnd_toEndOf="parent"
                    android:textAlignment="textEnd" android:layout_marginBottom="8dp"
                    card_view:layout_constraintBottom_toBottomOf="parent"/>
            <TextView android:layout_width="140dp" android:layout_height="wrap_content"
                      android:id="@+id/text_description_card_home"
                      card_view:layout_constraintTop_toBottomOf="@+id/text_account_num_card_home"
                      card_view:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
                      android:layout_marginBottom="8dp" card_view:layout_constraintBottom_toBottomOf="parent"
                      android:text="@string/text" android:textAlignment="center"/>
            <TextView android:layout_width="60dp" android:layout_height="wrap_content"
                      android:id="@+id/text_rate_account_card_home"
                      card_view:layout_constraintTop_toBottomOf="@+id/text_evaluation_card_home"
                      card_view:layout_constraintEnd_toEndOf="@+id/text_evaluation_card_home"
                      card_view:layout_constraintStart_toStartOf="@+id/text_evaluation_card_home"
                      card_view:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
                      android:text="@string/text" android:textAlignment="textEnd"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>

<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

    </data>
    <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:id="@+id/fragment_home_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
                                                           android:layout_height="wrap_content">
            <TextView
                    android:text="@string/account_number"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView29"
                    android:textAlignment="center" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="8dp"/>
            <TextView
                    android:text="@string/principal"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView30"
                    android:textAlignment="center"
                    app:layout_constraintStart_toEndOf="@+id/textView29" android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="@+id/textView29"
                    app:layout_constraintBottom_toBottomOf="@+id/textView29"/>
            <TextView
                    android:text="@string/evaluation"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView31"
                    android:textAlignment="center"
                    app:layout_constraintTop_toTopOf="@+id/textView30"
                    app:layout_constraintStart_toEndOf="@+id/textView30" android:layout_marginStart="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/textView30"/>
            <TextView
                    android:text="@string/profit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView32"
                    android:textAlignment="center"
                    app:layout_constraintStart_toEndOf="@+id/textView31" android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="@+id/textView31"
                    app:layout_constraintBottom_toBottomOf="@+id/textView31" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"/>
            <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerview_fragment_home_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:layout_editor_absoluteX="0dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView32"
                    app:layout_constraintBottom_toBottomOf="parent">

            </androidx.recyclerview.widget.RecyclerView>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</layout>
